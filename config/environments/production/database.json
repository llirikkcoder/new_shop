{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-mongoose",
      "settings": {
        "client": "mongo",
        "host": "${process.env.DATABASE_HOST || 'cluster0-vpsuu.mongodb.net'}",
        "port": "${process.env.DATABASE_PORT || 27017}",
        "database": "${process.env.DATABASE_NAME || 'server'}",
        "username": "${process.env.DATABASE_USERNAME || 'kipol'}",
        "password": "${process.env.DATABASE_PASSWORD || 'budda1985'}",
        "srv": "${process.env.DATABASE_SRV || true}"
      },
      "options": {
        "ssl": "${process.env.DATABASE_SSL || true}"
      }
    }
  }
}
